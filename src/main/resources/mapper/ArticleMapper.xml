<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.hicloud.radon.mapper.AutoIdMapper">


	<select id="get" resultType="com.example.demo.entity.Article">
		select id,
			   title,
			   author,
			   content
		from article where id=#{id}
	</select>


	<select id="getList" resultType="com.example.demo.entity.Article">
		select id,
			   title,
			   author,
			   content
		from article
	</select>

</mapper>
